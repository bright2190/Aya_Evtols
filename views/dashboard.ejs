<!DOCTYPE html>
<html lang="en"  data-bs-theme="light">
<head>
  <meta charset="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />

  <link rel="stylesheet" href="/css/dashboard_style.css" />
  <link rel="stylesheet" href="https://unicons.iconscout.com/release/v4.0.0/css/line.css" />

  <title>Dashboard - <%= project_name %></title>
</head>
<body>
  <%- include('./partials/__side_navbar.ejs') %>

  <section class="dashboard">
    <%- include('./partials/__topbar.ejs') %>

    <div class="dash-content">
      <div class="overview">
        <div class="title">
          <i class="uil uil-tachometer-fast-alt"></i>
          <span class="text">Dashboard</span>
        </div>

        <div class="boxes">
          <div class="box box1">
            <i class="uil uil-plane"></i>
            <span class="text">No. of eVTOLs</span>
            <span class="number">8</span>
          </div>
          <div class="box box2">
            <i class="uil uil-envelope-send"></i>
            <span class="text">Total Orders</span>
            <span class="number">266</span>
          </div>
          <div class="box box3">
            <i class="uil uil-plane-arrival"></i>
            <span class="text">Total Deliveries</span>
            <span class="number">249</span>
          </div>
        </div>
      </div>

      <div class="activity">
      </div>
    </div>
  </section>

  <script src="/js/app.js"></script>
  <script src="/js/localStorage.js"></script>
</body>
</html>
